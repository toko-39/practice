{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "ES2020.Symbol.WellKnown"],
    "module": "commonjs",
    "strict": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./",
    "rootDir": "./",
    "resolveJsonModule": true,
    "moduleResolution": "node"
  },
  "include": [
    "*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
